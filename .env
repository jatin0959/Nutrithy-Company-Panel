# Company Wellness Panel Environment Variables

# API Configuration
VITE_API_URL=http://localhost:3001/api
VITE_APP_NAME="Wellness Company Panel"
VITE_APP_VERSION=1.0.0

# Authentication
VITE_USE_MOCK_AUTH=true
VITE_JWT_SECRET=your-jwt-secret-key-here

# Company Configuration
VITE_COMPANY_NAME="TechCorp Solutions"
VITE_COMPANY_LOGO="TC"
VITE_COMPANY_PLAN="Enterprise"

# Features Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_TEAMS=true
VITE_ENABLE_CHALLENGES=true
VITE_ENABLE_REPORTS=true

# External Services (Optional)
VITE_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
VITE_SENTRY_DSN=your-sentry-dsn-here

# Development
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=info

# Production
# VITE_API_URL=https://your-production-api.com/api
# VITE_USE_MOCK_AUTH=false
# VITE_DEBUG_MODE=false